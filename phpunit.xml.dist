<?xml version="1.0" encoding="UTF-8"?>
<phpunit bootstrap="./vendor/autoload.php"
         colors="true">
  <testsuites>
    <testsuite>
      <directory>tests</directory>
    </testsuite>
  </testsuites>
  <filter>
    <whitelist suffix=".php" processUncoveredFilesFromWhitelist="true">
      <directory>src</directory>
    </whitelist>
  </filter>
</phpunit>
